{
  "name": "vite-plugin-instrumentation",
  "version": "0.0.10",
  "description": "The Vite plugin for configuring settings at build or server startup",
  "repository": {
    "type": "git",
    "url": "https://github.com/cut0/vite-plugin-instrumentation.git"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./*": "./*"
  },
  "scripts": {
    "lint:check": "biome ci",
    "lint:fix": "biome check --fix",
    "typecheck": "tsc --noEmit",
    "test": "vitest",
    "build": "tsc -p ./tsconfig.build.json && tsx build.ts",
    "release": "pnpm build && changeset publish"
  },
  "keywords": [
    "vite",
    "plugin",
    "instrumentation"
  ],
  "author": "Cut0",
  "license": "MIT",
  "files": [
    "dist"
  ],
  "dependencies": {
    "vite": "^5.4.2"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@changesets/changelog-github": "^0.5.0",
    "@changesets/cli": "^2.27.7",
    "@commitlint/cli": "^19.4.1",
    "@types/node": "^22.5.1",
    "esbuild": "^0.24.0",
    "tsx": "^4.19.0",
    "typescript": "^5.5.4",
    "vitest": "^2.0.5"
  },
  "packageManager": "pnpm@9.15.2"
}
